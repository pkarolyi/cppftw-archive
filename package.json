    "@types/react": "18.2.61",
    "@types/react-dom": "18.2.19",
    "autoprefixer": "10.4.18",
    "eslint": "8.57.0",
    "eslint-config-next": "14.1.1",
    "postcss": "8.4.35",
    "prisma": "5.10.2",
    "tailwindcss": "3.4.1",
